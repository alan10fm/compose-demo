<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.eafm.compose.demo">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ComposeDemo">
        <activity
            android:name=".ConstraintLayoutActivity"
            android:exported="true"
            android:label="@string/title_activity_constraint_layout"
            android:theme="@style/Theme.ComposeDemo" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".GridActivity"
            android:exported="true"
            android:label="@string/title_activity_grid"
            android:theme="@style/Theme.ComposeDemo">
        </activity>
        <activity
            android:name=".CustomLayoutActivity"
            android:exported="true"
            android:label="@string/title_activity_custom_layout"
            android:theme="@style/Theme.ComposeDemo"></activity>
        <activity
            android:name=".ListActivity"
            android:exported="true"
            android:label="@string/title_activity_list"
            android:theme="@style/Theme.ComposeDemo" />
        <activity
            android:name=".ScaffoldActivity"
            android:exported="true"
            android:label="@string/title_activity_scaffold"
            android:theme="@style/Theme.ComposeDemo" />
        <activity
            android:name=".LayoutComposeActivity"
            android:exported="true"
            android:label="@string/title_activity_layout_compose"
            android:theme="@style/Theme.ComposeDemo" />
        <activity
            android:name=".CodeLabsComposeBasicsActivity"
            android:exported="true"
            android:label="@string/title_activity_code_labs_compose_basics"
            android:theme="@style/Theme.ComposeDemo" />
        <activity
            android:name=".ComposeBasicsActivity"
            android:exported="true" />
    </application>

</manifest>
